package youtube;

public class onefortyone {

	public static void main(String[] args) {
		
		String x = "hello"; //instance of an object -- class String
		System.out.println(x.charAt(0));
		
		String Name = "Evan";
		
		String a = String.format("Hello %s", Name);
		
		System.out.println(a);
		
		System.out.println(Name.length());
		
	}

}
